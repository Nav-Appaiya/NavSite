<body>

{% include('@NavCMS/Default/menu.html.twig') %}

<div class="container">

    {% block body %}{% endblock %}

    {% block javascripts %}
        <script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
        <script type="text/javascript" src="{{ asset('bundles/navcms/js/bootstrap.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('bundles/navcms/js/bootswatch.js') }}"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/highlight.js/8.4/highlight.min.js"></script>
        <script>hljs.initHighlightingOnLoad();</script>
